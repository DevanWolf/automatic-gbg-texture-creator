const uint8_t NUM_TEXTURES = sizeof(images)/IMAGE_DATA_LEN;


const uint8_t NUM_COLORS = 135;
bool usedColors[NUM_COLORS];


// the setup function runs once when you press reset or power the board
void setup()
{
  // Set up button, pullup opposite to active state
  pinMode(START_PIN, INPUT_PULLDOWN);

  TUAbsMouse.begin();

  Serial.begin(115200);

  // wait until device mounted
  while( !USBDevice.mounted() ) {
    delay(10);
  }

  Serial.println("Setup complete!");
}


void loop()
{
  // poll gpio once each 10 ms
  delay(10);

  // Whether button is pressed
  bool btn_pressed = (digitalRead(START_PIN) == HIGH);

  // indicate that we're alive and working
  digitalWrite(LED_BUILTIN, btn_pressed ? LOW : HIGH);

  // nothing to do if button is not pressed
  if (!btn_pressed) return;


  //////////


  if (!TUAbsMouse.ready()) return;


  //////////


  for (uint16_t n=0; n<NUM_TEXTURES; n++) {
    // Rehome for every texture in case the screen is accidentally touched
    TUAbsMouse.home();

    // Create a new Texture Nodon and get ready to plot the pixels
    createTextureNodon(n);

    // Reset the processed colors indicators
    resetUsedColors();

    // Get first color from the image data.
    // It represents the most used color and is used as the Background color.
    uint8_t baseColor = pgm_read_byte_near(&images[n][0]);

    // As new textures has a transparent background we don't have to fill-bucket the canvas if the most used color is "transparent"
    if (baseColor != 47) {
      pickColor(baseColor);               // Get color from the Palette
      TUAbsMouse.clickMouse(958, 268);    // Mouse click on "Bucket Tool"
      TUAbsMouse.clickMouse(820, 280);    // Fill the background on the canvas
      TUAbsMouse.clickMouse(991, 196);    // Mouse click on "Pencil Tool"
    }

    // Remember that we already used this color
    usedColors[baseColor] = true;

    if (n==0) {
      // Only click on pixel brush first time when creating multiple textures
      TUAbsMouse.clickMouse(940, 375);    // Mouse click on "Pixel Brush"
    }

    for (uint16_t t=1; t<IMAGE_DATA_LEN; t++) {
      // get the target pixel color from the image data at the current index
      uint8_t targetPixelColor = pgm_read_byte_near(&images[n][t]);

      // only process the color if we haven't already processed it
      if (!usedColors[targetPixelColor]) {
        // Remember that we already used/processed this color
        usedColors[targetPixelColor] = true;

        // Pick the color from the Palette
        pickColor(targetPixelColor);

        // go through the remaining rows - wrapping around at the end of each line
        for (uint16_t c=t; c<IMAGE_DATA_LEN; c++) {
          uint16_t cPosX = (c-1) % TEXTURE_NUM_PIXELS;
          if (cPosX == 0) {
            // release the button - at the start of a row
            TUAbsMouse.mouseButtonRelease();
          }

          uint8_t currentPixelColor = pgm_read_byte_near(&images[n][c]);
          if (currentPixelColor == targetPixelColor) {
            // move to the pixel location - current pixel location matches target
            uint16_t cPosY = (c-1) / TEXTURE_NUM_PIXELS;
            TUAbsMouse.moveTo(340+(cPosX*8), 158+(cPosY*8));

            // make sure the button is pressed - current pixel color matches
            TUAbsMouse.mouseButtonPress();
          } else {
            // release the button - current pixel color doesn't match
            TUAbsMouse.mouseButtonRelease();
          }
        }

        // make sure the mouse is released at the end of each color run
        TUAbsMouse.mouseButtonRelease();
      }
    }

    // Done with this Texture Nodon
    clickCloseWindow();

    // Let the display finish updating
    delay(500);
  }

  while(1) {
    // Turn LED on (LOW on Seeeduino XIAO)
    digitalWrite(LED_BUILTIN, LOW);
    delay(750);
    // Turn LED off (HIGH on Seeeduino XIAO)
    digitalWrite(LED_BUILTIN, HIGH);
    delay(750);
  }
}


