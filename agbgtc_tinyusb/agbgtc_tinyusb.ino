  memset(usedColors, false, sizeof(usedColors));
}

ALWAYS_INLINE
void clickCloseWindow() {
  // Click on the "X" to exit the currently open window
  TUAbsMouse.clickMouse(1222, 56);
}

ALWAYS_INLINE
void createTextureNodon(uint8_t n) {
  // Move to magnify plus button
  TUAbsMouse.moveTo(948, 694);

  if (n==0) {
    // assume the worst case on the first pass and zoom in enough to force the auto scale
    for (byte i=0; i<16; i++) {
      TUAbsMouse.clickMouse();
      delay(30); // extra delay to allow UI to update
    }
  } else {
    // after the first case we just need to magnify enough to cause the auto scale again
    for (byte i=0; i<3; i++) {
      TUAbsMouse.clickMouse();
      delay(30); // extra delay to allow UI to update
    }
  }

  TUAbsMouse.clickMouse(362, 692);      // Objects
  TUAbsMouse.clickMouse(362, 332);      // Special Objects
  TUAbsMouse.clickMouse(488, 572);      // Texture
  delay(500); // A delay that is required to be sure the nodon gear icon is being displayed
  TUAbsMouse.clickMouse(550, 575);      // Edit Texture Gear icon
}


ALWAYS_INLINE
void pickColor(int16_t color_pos) {
  int column = color_pos % 15;
  int row = color_pos / 15;
  int pos_x = 84+(column*48)+((column/3)*16);
  int pos_y = 175+(row*48)+((row/3)*16);

  TUAbsMouse.clickMouse(1187, 544);     // Mouse click to open "Palette gear tool/Colour Palette"
  TUAbsMouse.clickMouse(pos_x, pos_y);  // Move mouse to color coords. and click on it
  clickCloseWindow();
}
